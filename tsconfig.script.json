{
  "compilerOptions": {
    "lib": ["es2019"],
    "module": "commonjs",
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "allowJs": true
  }
}
